   "source": [
    "from ptp import slave, master\n",
    "import threading\n",
    "\n",
    "ts = threading.Thread(target=slave.main)\n",
    "ts.setDaemon(True)\n",
    "tm = threading.Thread(target=master.main)\n",
    "tm.setDaemon(True)\n",
    "\n",
    "ts.start()\n",
    "tm.start()\n",
    "tm.join()\n",
    "ts.join()"
   ]
  }
